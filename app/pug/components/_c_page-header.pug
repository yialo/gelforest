mixin page-header(pageHeader_options)
  - const blockClassname = 'page-header';
  header(
    class=[
      blockClassname,
      `${blockClassname}--${pageHeader_options.id}`,
      'js-menu'
    ]
  )&attributes(attributes)
    +__div(blockClassname, 'logo')
      +logo()
    +__div(blockClassname, 'call')
      +call()
    +__div(blockClassname, 'menu-button')
      +menu-button()
    +__section(blockClassname, 'services')
    +__section(blockClassname, 'interesting')
